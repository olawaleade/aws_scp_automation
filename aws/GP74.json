{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowCloudTrailManagement",
        "Effect": "Allow",
        "Action": [
          "cloudtrail:CreateTrail",
          "cloudtrail:UpdateTrail",
          "cloudtrail:DeleteTrail",
          "cloudtrail:StartLogging",
          "cloudtrail:StopLogging"
        ],
        "Resource": "*"
      },
      {
        "Sid": "DenydisableAWSCloudTrail",
        "Action": [
          "cloudtrail:StopLogging",
          "cloudtrail:DeleteTrail"
        ],
        "Resource": "*",
        "Effect": "Deny"
      }
    ]
  }