{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AWSCloudTrailManagement",
        "Effect": "Deny",
        "Action": [
          "cloudtrail:StopLogging",
          "cloudtrail:DeleteTrail",
          "cloudtrail:PutEventSelectors",
          "cloudtrail:UpdateTrail"
        ],
        "Resource": "*",
        "Condition": {
        "StringEquals": {
          "aws:ResourceTag/ccoe": "true"
        }
      }
      }
    ]
  }